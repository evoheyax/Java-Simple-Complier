
void PrintBoolean(boolean x) {
    if (x)
       Print(1);
    else
       Print(0);
}

boolean MakeBool(int x) {
   return x != 0;
}

void main() {

   while(true) {
     int x =Read();
     int y =Read();
     PrintBoolean(x < y);
     PrintBoolean(x <= y);
     PrintBoolean(x > y);
     PrintBoolean(x >= y);
     PrintBoolean(x == y);
     PrintBoolean(x != y);
     Println();
     boolean a = MakeBool(x);
     boolean b = MakeBool(y);
     PrintBoolean(a);
     PrintBoolean(b);
     PrintBoolean(!a);
     PrintBoolean(!b);
     PrintBoolean(a && b);
     PrintBoolean(a || b);
     Println();
  }
}
